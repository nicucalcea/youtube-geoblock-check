{
    "manifest_version": 2,
    "name": "YouTube Georestriction Logger",
    "version": "1.0",
    "description": "Logs a message to the console when a georestricted video is visited on YouTube.",
    "permissions": [
        "activeTab",
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": [
            "https://www.youtube.com/watch*",
            "https://www.youtube.com/embed/*"
        ],
        "js": [
            "contentScript.js"
        ]
    }]
}